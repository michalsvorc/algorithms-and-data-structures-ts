{
  "moduleFileExtensions": [
    "ts",
    "json",
    "js"
  ],
  "preset": "ts-jest",
  "rootDir": ".",
  "testRegex": ".*\\.(spec|test)\\.ts$",
  "collectCoverageFrom": [
    "<rootDir>/src/**/*.ts"
  ],
  "coverageDirectory": "coverage",
  "coveragePathIgnorePatterns": [],
  "moduleNameMapper": {},
  "testPathIgnorePatterns": [],
  "modulePathIgnorePatterns": [
    "<rootDir>/dist/",
    "<rootDir>/node_modules/"
  ],
  "testEnvironment": "node",
  "setupFilesAfterEnv": []
}
